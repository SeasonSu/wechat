{
    "appenders": {
        "logger": {
            "type": "datefile",
            "filename": "./logs/datefile/",
            "alwaysIncludePattern": true,
            "pattern": "yyyy-MM-dd.log"
        },
        "console":{
            "type":"console"
        }
    },
    "categories": {
        "default": {
            "appenders": ["logger"],
            "level": "all"
        },
        "console":{
            "appenders": ["console"],
            "level": "all"
        }
    },
    "replaceConsole": true
}
